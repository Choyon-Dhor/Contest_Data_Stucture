#include <bits/stdc++.h>
using namespace std;
int main()
{
    int t,i=1;
    cin>>t;
    while (t--)
    {
        int n;
        cin>>n;
        float arr[n],sum=0;
        for (int i = 0; i < n; i++)
        {
            cin>>arr[i];
            sum+=arr[i];
        }
        float cgpa= sum/ static_cast<float>(n);
        cout <<fixed << setprecision(3) <<"Case "<<i<<": "<< cgpa << endl;
        i++;
    }
    return 0;
}
